<%= form_with(model: food) do |form| %>
  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name%>
  </div>

  <div class="field">
    <%= form.label :measurement_unit %>
    <%= form.text_field :measurement_unit%>
  </div>

  <div class="field">
    <%= form.label :price %>
    <%= form.text_field :price, type: "number" %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>